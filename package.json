{
  "name": "BDPlugins",
  "version": "1.0.0",
  "description": "A repository where I store all my BetterDiscord plugins.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarahkittyy/BDPlugins"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "foodoc": "^0.0.9",
    "jsdoc": "^3.6.4",
    "mocha": "^8.1.3",
    "raw-loader": "^4.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "node ./scripts/pack.js development 0PluginLibrary",
